{
  "name": "back_pain",
  "remarks": [
    "Causes of back pain adjusted to fit military population.  All considered mechanical in nature (https://www.ncbi.nlm.nih.gov/pubmed/17477101)",
    "",
    "Lumbar strain or sprain - 92.11%",
    "Herniated disk - 5.26%",
    "Spondylolisthesis - 2.63%"
  ],
  "states": {
    "Initial": {
      "type": "Initial",
      "direct_transition": "Wait_for_Back_Pain"
    },
    "Military_Check": {
      "type": "Simple",
      "conditional_transition": [
        {
          "transition": "Is_Military",
          "condition": {
            "condition_type": "Active Condition",
            "codes": [
              {
                "system": "SNOMED-CT",
                "code": 450031000124104,
                "display": "Military service member of air force (occupation)"
              }
            ]
          }
        },
        {
          "transition": "Is_Civilian"
        }
      ]
    },
    "Is_Military": {
      "type": "Simple",
      "direct_transition": "Call_Back_Pain_Rates_Submodule"
    },
    "Is_Civilian": {
      "type": "Simple",
      "direct_transition": "No_Back_Pain"
    },
    "Wait_for_Back_Pain": {
      "type": "Delay",
      "direct_transition": "Military_Check",
      "range": {
        "low": 10,
        "high": 14,
        "unit": "months"
      }
    },
    "Call_Back_Pain_Rates_Submodule": {
      "type": "CallSubmodule",
      "submodule": "readiness/back_pain_rates",
      "conditional_transition": [
        {
          "transition": "Back_Pain_Encounter",
          "condition": {
            "condition_type": "Attribute",
            "attribute": "Back_Pain",
            "operator": "is not nil"
          }
        },
        {
          "transition": "No_Back_Pain"
        }
      ]
    },
    "Back_Pain_Encounter": {
      "type": "Encounter",
      "encounter_class": "ambulatory",
      "reason": "Back_Pain",
      "codes": [
        {
          "system": "SNOMED-CT",
          "code": 737470001,
          "display": "Primary care management (procedure)"
        }
      ],
      "direct_transition": "End_Back_Pain_Encounter"
    },
    "No_Back_Pain": {
      "type": "Simple",
      "direct_transition": "Wait_for_Back_Pain"
    },
    "End_Back_Pain_Encounter": {
      "type": "EncounterEnd",
      "direct_transition": "Call_Back_Pain_Times_SubModule"
    },
    "Call_Back_Pain_Times_SubModule": {
      "type": "CallSubmodule",
      "submodule": "readiness/back_pain_time",
      "direct_transition": "Wait_for_Back_Pain"
    }
  }
}